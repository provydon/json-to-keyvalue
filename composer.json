{
    "name": "provydon/json-to-keyvalue",
    "description": "A Laravel Nova helper that converts JSON/arrays into read-only KeyValue fields.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Providence Ifeosame",
            "email": "providenceifeosame@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "autoload": {
        "psr-4": {
            "Provydon\\JsonToKeyvalue\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Provydon\\JsonToKeyvalue\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "laravel/pint": "^1.25"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/pint",
        "format:test": "vendor/bin/pint --test"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Provydon\\JsonToKeyvalue\\JsonToKeyvalueServiceProvider"
            ]
        }
    }
}
